        \u03C0\u03C1\u03BF\u03C3\u03C4\u03AD\u03B8\u03B7\u03BA\u03B5 \u03B4\u03B5\u03CD\u03C4\u03B5\u03C1\u03BF\u03C2
        \u03BC\u03BF\u03BD\u03CC\u03C7\u03C9\u03C1\u03BF\u03C2 \u03BD\u03B1\u03CC\u03C2
        \u03B1\u03C6\u03B9\u03B5\u03C1\u03C9\u03BC\u03AD\u03BD\u03BF\u03C2 \u03C3\u03C4\u03BF\u03BD
