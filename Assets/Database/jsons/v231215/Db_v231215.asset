            - key: en
              text: 
            poi_short_description:
            - key: gr
              text: "\u03A4\u03BF \u0392\u0394 \u03C4\u03BC\u03AE\u03BC\u03B1 \u03C4\u03BF\u03C5
                \u03BA\u03B5\u03BD\u03C4\u03C1\u03B9\u03BA\u03BF\u03CD \u03BA\u03C4\u03B7\u03C1\u03AF\u03BF\u03C5
                \u03C3\u03C4\u03B7 \u0396\u03CE\u03BC\u03B9\u03BD\u03B8\u03BF \u03C0\u03B5\u03C1\u03B9\u03BB\u03B1\u03BC\u03B2\u03AC\u03BD\u03B5\u03B9
                \u03C4\u03BF\u03C5\u03C2 \u03C7\u03CE\u03C1\u03BF\u03C5\u03C2 \u03C3\u03C4\u03B1\u03B2\u03CC\u03C1\u03B5\u03B9\u03B1
                \u03BA\u03B1\u03B9 \u03B4\u03C5\u03C4\u03B9\u03BA\u03AC\u03C4\u03BF\u03C5 
                \u039A\u03B5\u03BD\u03C4\u03C1\u03B9\u03BA\u03BF\u03CD \u039A\u03C4\u03B9\u03C1\u03AF\u03BF\u03C5
                \u03BA\u03B1\u03B9 \u03B4\u03C5\u03C4\u03B9\u03BA\u03AC \u03C4\u03BF\u03C5
                \u0394\u03B9\u03B1\u03B4\u03C1\u03CC\u03BC\u03BF\u03C5 10."
            - key: en
              text: 
            poi_description:
            - key: gr
              text: "\u03A3\u03B7\u03BC\u03B1\u03BD\u03C4\u03B9\u03BA\u03CC \u03B5\u03BA\u03B5\u03AF
                \u03B5\u03AF\u03BD\u03B1\u03B9 \u03C4\u03BF \u03C3\u03C5\u03B3\u03BA\u03C1\u03CC\u03C4\u03B7\u03BC\u03B1
                \u03B4\u03C9\u03BC\u03B1\u03C4\u03AF\u03C9\u03BD 13-16, \u03C4\u03B1
                \u03B4\u03C9\u03BC\u03AC\u03C4\u03B9\u03B1 11-12, \u03BA\u03B1\u03B8\u03CE\u03C2
                \u03BA\u03B1\u03B9 \u03BF\u03B9 \u03B4\u03BF\u03BC\u03AD\u03C2 \u03B3\u03CD\u03C1\u03C9
                \u03B1\u03C0\u03CC \u03C4\u03BF\u03BD \u03BA\u03B5\u03C1\u03B1\u03BC\u03B5\u03B9\u03BA\u03CC
                \u03BA\u03BB\u03AF\u03B2\u03B1\u03BD\u03BF \u03C3\u03C4\u03B1 \u0392\u0394.\u039F
                \u03C4\u03C5\u03C6\u03BB\u03CC\u03C2 \u03A7\u03CE\u03C1\u03BF\u03C2
                13 \u03B1\u03C0\u03BF\u03C4\u03B5\u03BB\u03BF\u03CD\u03C3\u03B5 \u03BA\u03B5\u03C1\u03B1\u03BC\u03B5\u03B9\u03BA\u03CC
                \u03B5\u03C1\u03B3\u03B1\u03C3\u03C4\u03AE\u03C1\u03B9\u03BF \u03BA\u03B1\u03B9
                \u03C0\u03B5\u03C1\u03B9\u03B5\u03BB\u03AC\u03BC\u03B2\u03B1\u03BD\u03B5
                \u03BA\u03C4\u03B9\u03C3\u03C4\u03AE \u03B4\u03B5\u03BE\u03B1\u03BC\u03B5\u03BD\u03AE,
                \u03B4\u03CD\u03BF \u03B3\u03BF\u03CD\u03C1\u03BD\u03B5\u03C2 \u03B5\u03BD\u03C4\u03CC\u03C2
                \u03C4\u03C9\u03BD \u03BF\u03C0\u03BF\u03AF\u03C9\u03BD \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03C4\u03B7\u03BA\u03B5
                \u03B1\u03C1\u03C7\u03B1\u03AF\u03BF\u03C2 \u03C0\u03B7\u03BB\u03CC\u03C2
                \u03BA\u03B1\u03B9 \u03AD\u03BD\u03B1\u03C2 \u03BA\u03B5\u03C1\u03B1\u03BC\u03B5\u03B9\u03BA\u03CC\u03C2
                \u03C4\u03C1\u03BF\u03C7\u03CC\u03C2 \u03B1\u03BB\u03BB\u03AC \u03BA\u03B1\u03B9
                250 \u03B1\u03B3\u03B3\u03B5\u03AF\u03B1 23 \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03CE\u03BD
                \u03C3\u03C7\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD. \u03A3\u03C4\u03BF
                \u03A7\u03CE\u03C1\u03BF 14 \u03B1\u03C0\u03BF\u03BA\u03B1\u03BB\u03CD\u03C6\u03B8\u03B7\u03BA\u03B1\u03BD
                100 \u03B1\u03B3\u03B3\u03B5\u03AF\u03B1 \u03BC\u03B5\u03C4\u03B1\u03BE\u03CD
                \u03BA\u03B1\u03BC\u03AD\u03BD\u03C9\u03BD \u03BE\u03CD\u03BB\u03C9\u03BD,
                \u03BF\u03C3\u03C4\u03CE\u03BD \u03BA\u03B1\u03B9 \u03C5\u03C0\u03BF\u03BB\u03B5\u03B9\u03BC\u03BC\u03AC\u03C4\u03C9\u03BD
                \u03BA\u03B1\u03C1\u03C0\u03CE\u03BD. \u039F \u03A7\u03CE\u03C1\u03BF\u03C2
                15 \u03B5\u03AF\u03C7\u03B5 \u03BC\u03AC\u03BB\u03BB\u03BF\u03BD
                \u03BB\u03B1\u03C4\u03C1\u03B5\u03C5\u03C4\u03B9\u03BA\u03CC \u03C7\u03B1\u03C1\u03B1\u03BA\u03C4\u03AE\u03C1\u03B1
                \u03BA\u03B1\u03B8\u03CE\u03C2 \u03C3\u03B5 \u03BA\u03CC\u03B3\u03C7\u03B7
                \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03C4\u03B7\u03BA\u03B1\u03BD
                \u03C4\u03C1\u03AF\u03B1 \u03BB\u03B5\u03C0\u03C4\u03CC\u03C4\u03B5\u03C7\u03BD\u03B1
                \u03BA\u03CD\u03C0\u03B5\u03BB\u03BB\u03B1 \u03BA\u03BF\u03B9\u03BD\u03C9\u03BD\u03AF\u03B1\u03C2/
                \u03C1\u03C5\u03C4\u03AC \u03BC\u03B5 \u03B4\u03B9\u03B1\u03BA\u03CC\u03C3\u03BC\u03B7\u03C3\u03B7
                \u03BA\u03B1\u03BB\u03B1\u03BC\u03BF\u03B5\u03B9\u03B4\u03CE\u03BD
                \u03BA\u03B1\u03B8\u03CE\u03C2 \u03BA\u03B1\u03B9 \u03C4\u03BC\u03AE\u03BC\u03B1
                \u03B1\u03C0\u03CC \u03BB\u03AF\u03B8\u03B9\u03BD\u03B1 \u03BA\u03AD\u03C1\u03B1\u03C4\u03B1
                \u03BA\u03B1\u03B8\u03BF\u03C3\u03B9\u03CE\u03C3\u03B5\u03C9\u03C2.
                \u03A3\u03C4\u03BF\u03BD \u03CC\u03C1\u03BF\u03C6\u03BF \u03C4\u03BF\u03C5
                \u03A7\u03CE\u03C1\u03BF\u03C5 15 \u03B7 \u03C0\u03B1\u03C1\u03BF\u03C5\u03C3\u03AF\u03B1
                \u03BF\u03C1\u03B5\u03AF\u03B1\u03C2 \u03BA\u03C1\u03C5\u03C3\u03C4\u03AC\u03BB\u03BB\u03BF\u03C5
                \u03BA\u03B1\u03B9 \u03B5\u03C1\u03B3\u03B1\u03BB\u03B5\u03AF\u03C9\u03BD
                \u03C0\u03B1\u03C1\u03B1\u03C0\u03AD\u03BC\u03C0\u03B5\u03B9 \u03C3\u03C4\u03B7\u03BD
                \u03CD\u03C0\u03B1\u03C1\u03BE\u03B7 \u03B5\u03C1\u03B3\u03B1\u03C3\u03C4\u03B7\u03C1\u03AF\u03BF\u03C5
                \u03BB\u03AF\u03B8\u03B9\u03BD\u03C9\u03BD \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03B9\u03BC\u03AD\u03BD\u03C9\u03BD.
                \u0391\u03BD\u03B1\u03C4\u03BF\u03BB\u03B9\u03BA\u03AC \u03C4\u03BF\u03C5
                \u03A7\u03CE\u03C1\u03BF\u03C5 15 \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03B6\u03BF\u03BD\u03C4\u03B1\u03B9
                \u03BF\u03B9 \u03A7\u03CE\u03C1\u03BF\u03B9 11-12. \u03A3\u03C4\u03BF
                \u0392\u03CC\u03C1\u03B5\u03B9\u03BF \u03C4\u03BF\u03AF\u03C7\u03BF
                \u03C4\u03BF\u03C5 \u03A7\u03CE\u03C1\u03BF\u03C5 11 \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03C4\u03B7\u03BA\u03B5 
                \u03BA\u03CC\u03B3\u03C7\u03B7 \u03C3\u03C4\u03BF \u03B9\u03C3\u03CC\u03B3\u03B5\u03B9\u03BF,
                \u03CC\u03C0\u03BF\u03C5 \u03B2\u03C1\u03AD\u03B8\u03B7\u03BA\u03B1\u03BD
                \u03C1\u03C5\u03C4\u03CC \u03C3\u03B5 \u03C3\u03C7\u03AE\u03BC\u03B1
                \u03C7\u03BF\u03AF\u03C1\u03BF\u03C5, \u03C0\u03CC\u03B4\u03B9\u03B1
                \u03B5\u03B9\u03B4\u03C9\u03BB\u03AF\u03BF\u03C5 \u03C4\u03B1\u03CD\u03C1\u03BF\u03C5
                \u03BA\u03B1\u03B8\u03CE\u03C2 \u03BA\u03B1\u03B9 \u03B1\u03B3\u03B3\u03B5\u03AF\u03B1
                \u03C0\u03BF\u03C5 \u03C5\u03C0\u03BF\u03B4\u03B7\u03BB\u03CE\u03BD\u03BF\u03C5\u03BD
                \u03B5\u03B9\u03B4\u03B9\u03BA\u03AE \u03C7\u03C1\u03AE\u03C3\u03B7
                \u03C4\u03BF\u03C5 \u03C7\u03CE\u03C1\u03BF\u03C5. \u03A3\u03C4\u03BF\u03BD
                \u03CC\u03BC\u03BF\u03C1\u03BF \u03A7\u03CE\u03C1\u03BF 12 \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03C4\u03B7\u03BA\u03B1\u03BD
                \u03C0\u03AE\u03BB\u03B9\u03BD\u03B1 \u03B1\u03BD\u03C4\u03B9\u03BA\u03B5\u03AF\u03BC\u03B5\u03BD\u03B1
                \u03B5\u03BD \u03B5\u03AF\u03B4\u03B7 \u03BA\u03CD\u03BC\u03B2\u03B7\u03C2,
                \u03BC\u03AE\u03BA\u03BF\u03C5\u03C2 1\u03BC.\u03A0\u03BF\u03BB\u03CD
                \u03B5\u03BD\u03B4\u03B9\u03B1\u03C6\u03AD\u03C1\u03C9\u03BD \u03B5\u03AF\u03BD\u03B1\u03B9
                \u03BF \u03A7\u03CE\u03C1\u03BF\u03C2 58 \u03C0\u03BF\u03C5 \u03C3\u03C4\u03BF\u03BD
                \u03AC\u03BD\u03C9 \u03CC\u03C1\u03BF\u03C6\u03BF \u03C0\u03B5\u03C1\u03B9\u03B5\u03AF\u03C7\u03B5
