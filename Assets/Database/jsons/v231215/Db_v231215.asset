          \u03C0\u03B5\u03C1\u03B9\u03B5\u03BB\u03AC\u03BC\u03B2\u03B1\u03BD\u03B5
          \u03BA\u03C4\u03B9\u03C3\u03C4\u03AE \u03B4\u03B5\u03BE\u03B1\u03BC\u03B5\u03BD\u03AE,
          \u03B4\u03CD\u03BF \u03B3\u03BF\u03CD\u03C1\u03BD\u03B5\u03C2 \u03B5\u03BD\u03C4\u03CC\u03C2
          \u03C4\u03C9\u03BD \u03BF\u03C0\u03BF\u03AF\u03C9\u03BD \u03B5\u03BD\u03C4\u03BF\u03C0\u03AF\u03C3\u03C4\u03B7\u03BA\u03B5
          \u03B1\u03C1\u03C7\u03B1\u03AF\u03BF\u03C2 \u03C0\u03B7\u03BB\u03CC\u03C2
          \u03BA\u03B1\u03B9 \u03AD\u03BD\u03B1\u03C2 \u03BA\u03B5\u03C1\u03B1\u03BC\u03B5\u03B9\u03BA\u03CC\u03C2
          \u03C4\u03C1\u03BF\u03C7\u03CC\u03C2 \u03B1\u03BB\u03BB\u03AC \u03BA\u03B1\u03B9
          250 \u03B1\u03B3\u03B3\u03B5\u03AF\u03B1 23 \u03B4\u03B9\u03B1\u03C6\u03BF\u03C1\u03B5\u03C4\u03B9\u03BA\u03CE\u03BD
          \u03C3\u03C7\u03B7\u03BC\u03AC\u03C4\u03C9\u03BD. \u03A3\u03C4\u03BF \u03A7\u03CE\u03C1\u03BF
          14 \u03B1\u03C0\u03BF\u03BA\u03B1\u03BB\u03CD\u03C6\u03B8\u03B7\u03BA\u03B1\u03BD
          100 \u03B1\u03B3\u03B3\u03B5\u03AF\u03B1 \u03BC\u03B5\u03C4\u03B1\u03BE\u03CD
          \u03BA\u03B1\u03BC\u03AD\u03BD\u03C9\u03BD \u03BE\u03CD\u03BB\u03C9\u03BD,
          \u03BF\u03C3\u03C4\u03CE\u03BD \u03BA\u03B1\u03B9 \u03C5\u03C0\u03BF\u03BB\u03B5\u03B9\u03BC\u03BC\u03AC\u03C4\u03C9\u03BD
