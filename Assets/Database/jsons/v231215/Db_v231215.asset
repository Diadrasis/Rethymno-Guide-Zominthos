                \u03C0\u03BF\u03BB\u03C5\u03C4\u03B5\u03BB\u03B5\u03AF\u03C2 \u03B9\u03B4\u03B9\u03C9\u03C4\u03B9\u03BA\u03BF\u03CD\u03C2
                \u03C7\u03CE\u03C1\u03BF\u03C5\u03C2 \u03B4\u03B9\u03B1\u03BC\u03BF\u03BD\u03AE\u03C2,
                \u03C3\u03C5\u03C7\u03BD\u03AC \u03C4\u03BF\u03B9\u03C7\u03BF\u03B3\u03C1\u03B1\u03C6\u03B7\u03BC\u03AD\u03BD\u03BF\u03C5\u03C2,
                \u03BA\u03BB\u03AF\u03BC\u03B1\u03BA\u03B5\u03C2, \u03C6\u03C9\u03C4\u03B1\u03B3\u03C9\u03B3\u03BF\u03CD\u03C2,
                \u03BC\u03B1\u03B3\u03B5\u03B9\u03C1\u03B5\u03AF\u03B1, \u03B1\u03C0\u03BF\u03B8\u03B7\u03BA\u03B5\u03C5\u03C4\u03B9\u03BA\u03BF\u03CD\u03C2
                \u03C7\u03CE\u03C1\u03BF\u03C5\u03C2, \u03B1\u03C5\u03BB\u03AE, \u03B5\u03C1\u03B3\u03B1\u03C3\u03C4\u03AE\u03C1\u03B9\u03B1
                \u03BA\u03B5\u03C1\u03B1\u03BC\u03B5\u03B9\u03BA\u03AE\u03C2 \u03BA\u03B1\u03B9 
                \u03BB\u03AF\u03B8\u03BF\u03C5 (\u03BF\u03C1\u03B5\u03AF\u03B1\u03C2
                \u03BA\u03C1\u03C5\u03C3\u03C4\u03AC\u03BB\u03BB\u03BF\u03C5) \u03BA\u03B1\u03B9
                \u03BF\u03C3\u03C4\u03CE\u03BD,  \u03BC\u03B5\u03C4\u03B1\u03BB\u03BB\u03BF\u03C5\u03C1\u03B3\u03AF\u03B1\u03C2,
                \u03C7\u03CE\u03C1\u03BF\u03C5\u03C2 \u03BB\u03B1\u03C4\u03C1\u03B5\u03AF\u03B1\u03C2
                \u03AE \u03BF\u03BC\u03B1\u03B4\u03B9\u03BA\u03CE\u03BD \u03C3\u03C5\u03B3\u03BA\u03B5\u03BD\u03C4\u03C1\u03CE\u03C3\u03B5\u03C9\u03BD
                \u03BA\u03B1\u03B8\u03CE\u03C2 \u03BA\u03B1\u03B9 \u03B8\u03C5\u03C1\u03C9\u03C1\u03B5\u03AF\u03BF.(CL)\u03A4\u03B1
                \u03B1\u03C1\u03C7\u03B9\u03C4\u03B5\u03BA\u03C4\u03BF\u03BD\u03B9\u03BA\u03AC
                \u03C7\u03B1\u03C1\u03B1\u03BA\u03C4\u03B7\u03C1\u03B9\u03C3\u03C4\u03B9\u03BA\u03AC
                \u03BA\u03B1\u03B9 \u03C4\u03B1 \u03B5\u03C5\u03C1\u03AE\u03BC\u03B1\u03C4\u03B1
                \u03C4\u03B7\u03C2 \u0396\u03C9\u03BC\u03AF\u03BD\u03B8\u03BF\u03C5
                \u03B4\u03B5\u03AF\u03C7\u03BD\u03BF\u03C5\u03BD \u03CC\u03C4\u03B9
                \u03C0\u03C1\u03CC\u03BA\u03B5\u03B9\u03C4\u03B1\u03B9 \u03B3\u03B9\u03B1
                \u03AD\u03BD\u03B1 \xAB\u03B1\u03BD\u03AC\u03BA\u03C4\u03BF\u03C1\u03BF
                \u03C3\u03C4\u03BF \u03B2\u03BF\u03C5\u03BD\u03CC\xBB \u03B7 \u03C0\u03B1\u03C1\u03BF\u03C5\u03C3\u03AF\u03B1
                \u03C4\u03BF\u03C5 \u03BF\u03C0\u03BF\u03AF\u03BF\u03C5 \u03C3\u03C5\u03BD\u03B4\u03B5\u03CC\u03C4\u03B1\u03BD
                \u03BC\u03B5 \u03BC\u03AF\u03B1 \u03C3\u03C4\u03B9\u03B2\u03B1\u03C1\u03AE
                \u03BA\u03C4\u03B7\u03BD\u03BF\u03C4\u03C1\u03BF\u03C6\u03B9\u03BA\u03AE
                \u03BF\u03B9\u03BA\u03BF\u03BD\u03BF\u03BC\u03AF\u03B1, \u03C0\u03BF\u03C5
                \u03C0\u03C1\u03BF\u03AD\u03BA\u03C5\u03C0\u03C4\u03B5 \u03B1\u03C0\u03CC
                \u03C4\u03B7\u03BD \u03B5\u03BA\u03BC\u03B5\u03C4\u03AC\u03BB\u03BB\u03B5\u03C5\u03C3\u03B7
                \u03C4\u03BF\u03C5 \u03C6\u03C5\u03C3\u03B9\u03BA\u03BF\u03CD \u03C0\u03BB\u03BF\u03CD\u03C4\u03BF\u03C5
                \u03C4\u03BF\u03C5 \u03B2\u03BF\u03C5\u03BD\u03BF\u03CD \u03B1\u03BB\u03BB\u03AC
                \u03BA\u03B1\u03B9 \u03C4\u03B7\u03BD \u03B4\u03B9\u03B1\u03C7\u03B5\u03AF\u03C1\u03B9\u03C3\u03B7
                \u03C4\u03BF\u03C5 \u03B9\u03B5\u03C1\u03BF\u03CD \u03C3\u03C0\u03B7\u03BB\u03B1\u03AF\u03BF\u03C5
                \u03C4\u03BF\u03C5 \u0399\u03B4\u03B1\u03AF\u03BF\u03C5 \u0386\u03BD\u03C4\u03C1\u03BF\u03C5.(CL)\u0397
                \u03B1\u03BD\u03B1\u03BA\u03C4\u03BF\u03C1\u03B9\u03BA\u03AE \u03B5\u03B3\u03BA\u03B1\u03C4\u03AC\u03C3\u03C4\u03B1\u03C3\u03B7
                \u03C4\u03B7\u03C2 \u0396\u03C9\u03BC\u03AF\u03BD\u03B8\u03BF\u03C5
                \u03BA\u03B1\u03C4\u03B1\u03C3\u03C4\u03C1\u03AC\u03C6\u03B7\u03BA\u03B5
                \u03B4\u03CD\u03BF \u03C6\u03BF\u03C1\u03AD\u03C2 \u03B1\u03C0\u03CC
                \u03C3\u03B5\u03B9\u03C3\u03BC\u03CC \u03BA\u03B1\u03C4\u03AC \u03C4\u03B7
                \u03B4\u03B9\u03AC\u03C1\u03BA\u03B5\u03B9\u03B1 \u03C4\u03C9\u03BD
                \u039C\u03B9\u03BD\u03C9\u03B9\u03BA\u03CE\u03BD \u03C7\u03C1\u03CC\u03BD\u03C9\u03BD."
